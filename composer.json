  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^7.5"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.5",
    "fakerphp/faker": "^1.20"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Kevin Em",
      "email": "kevinem@thinkdevise.com"
    },
    {
      "name": "palPalani",
      "email": "palani.p@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "KevinEm\\LimeLightCRM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KevinEm\\LimeLightCRM\\Tests\\": "tests/"
    }
  },
  "scripts": {
      "format": "vendor/bin/pint"
